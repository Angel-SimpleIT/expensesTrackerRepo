{
  "projectName": "Sequence Financial Dashboard",
  "version": "1.0.0",
  "metadata": {
    "description": "A clean, modern fintech interface utilizing a dark teal and bright green colorway with high-radius cards and Helvetica typography.",
    "baseUnit": "4px",
    "createdAt": "2024-05-20"
  },
  "designSystem": {
    "layout": {
      "grid": {
        "columns": 12,
        "gutter": "24px",
        "margin": "32px",
        "maxWidth": "1600px",
        "breakpoints": {
          "mobile": "375px",
          "tablet": "768px",
          "desktop": "1024px",
          "wide": "1440px"
        }
      },
      "containers": {
        "dashboard": {
          "maxWidth": "100%",
          "padding": "32px",
          "backgroundColor": "#F4F6F8"
        },
        "card": {
          "padding": "24px",
          "borderRadius": "24px",
          "backgroundColor": "#FFFFFF"
        }
      },
      "zIndices": {
        "base": 1,
        "dropdown": 100,
        "stickyNav": 500,
        "modal": 1000,
        "tooltip": 1100
      }
    },
    "typography": {
      "fontFamily": {
        "primary": "Helvetica, 'Helvetica Neue', Arial, sans-serif",
        "fallback": "sans-serif"
      },
      "weights": {
        "regular": 400,
        "medium": 500,
        "semiBold": 600,
        "bold": 700
      },
      "scale": {
        "display1": {
          "size": "32px",
          "lineHeight": "1.2",
          "weight": 500,
          "letterSpacing": "-0.02em"
        },
        "h1": {
          "size": "24px",
          "lineHeight": "1.3",
          "weight": 700,
          "letterSpacing": "-0.01em"
        },
        "h2": {
          "size": "20px",
          "lineHeight": "1.4",
          "weight": 600,
          "letterSpacing": "-0.01em"
        },
        "h3": {
          "size": "16px",
          "lineHeight": "1.5",
          "weight": 600,
          "letterSpacing": "0em"
        },
        "bodyLarge": {
          "size": "16px",
          "lineHeight": "1.5",
          "weight": 400,
          "letterSpacing": "0em"
        },
        "body": {
          "size": "14px",
          "lineHeight": "1.5",
          "weight": 400,
          "letterSpacing": "0em"
        },
        "caption": {
          "size": "12px",
          "lineHeight": "1.4",
          "weight": 500,
          "letterSpacing": "0.02em",
          "textTransform": "none"
        },
        "moneyLarge": {
          "size": "42px",
          "lineHeight": "1",
          "weight": 400,
          "letterSpacing": "-0.03em"
        }
      }
    },
    "colors": {
      "primary": {
        "main": "#025864",
        "dark": "#013E47",
        "light": "#037585",
        "surface": "#E6EEF0",
        "contrastText": "#FFFFFF"
      },
      "accent": {
        "main": "#00D47E",
        "hover": "#00B86D",
        "light": "#CCF6E5",
        "subtle": "#E5FAF2",
        "contrastText": "#025864"
      },
      "neutral": {
        "900": "#111827",
        "800": "#1F2937",
        "700": "#374151",
        "600": "#4B5563",
        "500": "#6B7280",
        "400": "#9CA3AF",
        "300": "#D1D5DB",
        "200": "#E5E7EB",
        "100": "#F3F4F6",
        "50": "#F9FAFB",
        "white": "#FFFFFF"
      },
      "semantic": {
        "success": {
          "text": "#00D47E",
          "bg": "#E5FAF2"
        },
        "error": {
          "text": "#EF4444",
          "bg": "#FEF2F2"
        },
        "warning": {
          "text": "#F59E0B",
          "bg": "#FFFBEB"
        }
      },
      "charts": {
        "barPrimary": "#025864",
        "barSecondary": "#00D47E",
        "gridLine": "#E5E7EB"
      },
      "background": {
        "default": "#F0F2F5",
        "paper": "#FFFFFF",
        "dark": "#025864"
      }
    },
    "spacing": {
      "xs": "4px",
      "sm": "8px",
      "md": "16px",
      "lg": "24px",
      "xl": "32px",
      "xxl": "48px"
    },
    "borders": {
      "radius": {
        "sm": "8px",
        "md": "12px",
        "lg": "16px",
        "xl": "24px",
        "full": "9999px"
      },
      "width": {
        "thin": "1px",
        "thick": "2px"
      },
      "color": {
        "default": "#E5E7EB",
        "focus": "#00D47E"
      }
    },
    "effects": {
      "shadows": {
        "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
        "card": "0 4px 24px 0 rgba(0, 0, 0, 0.04)",
        "float": "0 10px 30px -5px rgba(2, 88, 100, 0.15)",
        "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.06)"
      },
      "blur": {
        "backdrop": "blur(8px)"
      },
      "transitions": {
        "default": "all 0.2s cubic-bezier(0.4, 0, 0.2, 1)",
        "hover": "all 0.3s ease-out"
      }
    }
  },
  "components": {
    "header": {
      "height": "90px",
      "backgroundColor": "#025864",
      "padding": "0 32px",
      "borderRadius": "0 0 24px 24px",
      "elements": {
        "totalBalanceLabel": {
          "color": "rgba(255,255,255,0.7)",
          "fontSize": "14px",
          "marginBottom": "4px"
        },
        "totalBalanceValue": {
          "color": "#FFFFFF",
          "fontSize": "32px",
          "fontWeight": 500
        },
        "percentage": {
          "color": "#00D47E",
          "fontSize": "14px",
          "marginLeft": "8px"
        }
      }
    },
    "buttons": {
      "primary": {
        "backgroundColor": "#00D47E",
        "color": "#FFFFFF",
        "borderRadius": "12px",
        "padding": "10px 20px",
        "fontSize": "14px",
        "fontWeight": 500,
        "boxShadow": "0 2px 8px rgba(0, 212, 126, 0.25)",
        "hover": {
          "transform": "translateY(-1px)",
          "backgroundColor": "#00C272"
        }
      },
      "ghost": {
        "backgroundColor": "rgba(255, 255, 255, 0.1)",
        "color": "#FFFFFF",
        "borderRadius": "12px",
        "padding": "10px 20px",
        "fontSize": "14px",
        "border": "1px solid rgba(255, 255, 255, 0.15)",
        "hover": {
          "backgroundColor": "rgba(255, 255, 255, 0.2)"
        }
      },
      "filter": {
        "backgroundColor": "#FFFFFF",
        "color": "#111827",
        "border": "1px solid #E5E7EB",
        "borderRadius": "8px",
        "padding": "6px 12px",
        "fontSize": "13px"
      },
      "iconButton": {
        "size": "32px",
        "backgroundColor": "rgba(255, 255, 255, 0.1)",
        "color": "#FFFFFF",
        "borderRadius": "8px",
        "display": "flex",
        "alignItems": "center",
        "justifyContent": "center"
      }
    },
    "cards": {
      "default": {
        "backgroundColor": "#FFFFFF",
        "borderRadius": "24px",
        "boxShadow": "0 4px 24px 0 rgba(0, 0, 0, 0.04)",
        "padding": "24px"
      },
      "darkCreditCard": {
        "background": "linear-gradient(135deg, #025864 0%, #013E47 100%)",
        "color": "#FFFFFF",
        "borderRadius": "20px",
        "padding": "24px",
        "aspectRatio": "1.586",
        "position": "relative",
        "overlayPattern": "radial-gradient(circle at 100% 100%, rgba(255,255,255,0.1) 0%, transparent 50%)"
      }
    },
    "charts": {
      "bar": {
        "height": "180px",
        "barWidth": "12px",
        "borderRadius": "4px",
        "colors": [
          "#025864",
          "#00D47E"
        ],
        "spacing": "16px"
      },
      "sparkline": {
        "strokeWidth": 2,
        "color": "#00D47E"
      }
    },
    "transactionList": {
      "row": {
        "height": "72px",
        "borderBottom": "1px solid #F3F4F6",
        "display": "flex",
        "alignItems": "center",
        "justifyContent": "space-between"
      },
      "avatar": {
        "size": "40px",
        "borderRadius": "50%",
        "backgroundColor": "#F3F4F6"
      },
      "statusBadge": {
        "padding": "4px 12px",
        "borderRadius": "6px",
        "fontSize": "12px",
        "fontWeight": 500,
        "success": {
          "bg": "#E5FAF2",
          "color": "#00D47E"
        },
        "pending": {
          "bg": "#F3F4F6",
          "color": "#6B7280"
        }
      }
    }
  }
}